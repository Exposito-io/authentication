{
  "name": "typescript-express-rest-seed",
  "version": "0.0.1",
  "description": "An express REST API seed written in TypeScript.",
  "main": "bin/www.ts",
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./build/bin/www.js",
    "predev": "npm run clean",
    "dev": "concurrently \"npm run tsc-watch\" \"npm run nodemon\"",
    "prenodemon": "wait-on ./build/bin/www.js",
    "nodemon": "nodemon --delay 1500ms --watch ./build \"./build/bin/www.js\"",
    "tsc-watch": "tsc -w",
    "lint": "tslint -c tslint.json \"./src/**/*.ts\""
  },
  "author": "Andre Giannico <andre@andregiannico.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/giannico/typescript-express-rest-seed.git"
  },
  "keywords": [
    "express",
    "typescript",
    "rest",
    "api",
    "types"
  ],
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "dependencies": {
    "app-root-path": "^1.3.0",
    "body-parser": "1.15.2",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "1.4.3",
    "core-js": "2.4.1",
    "ejs": "2.5.1",
    "express": "4.14.0",
    "express-session": "^1.15.2",
    "morgan": "1.7.0",
    "passport": "^0.3.2",
    "passport-google-oauth2": "^0.1.6",
    "serialize-error": "2.0.0",
    "validate.js": "0.10.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/body-parser": "0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/core-js": "0.9.34",
    "@types/debug": "0.0.29",
    "@types/express": "4.0.33",
    "@types/morgan": "1.7.32",
    "@types/validator": "5.7.34",
    "@types/winston": "0.0.28",
    "concurrently": "3.1.0",
    "dotenv": "2.0.0",
    "nodemon": "1.11.0",
    "rimraf": "2.5.4",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "wait-on": "1.5.3"
  }
}
